	},
	"license": "gpl-3.0",
	"keywords": [
		"directus",
		"directus-extension",
		"directus-custom-interface",
		"masked-interface",
		"masked-input"
	],
	"directus:extension": {
		"type": "interface",
		"path": "dist/index.js",
		"source": "src/index.js",
